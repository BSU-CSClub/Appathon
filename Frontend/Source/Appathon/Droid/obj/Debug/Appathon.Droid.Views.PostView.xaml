<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Appathon.PostView">
	<ContentView.Content>
		<StackLayout Padding="5">
			<Label Text="{Binding PostModel.Title}" />
			<Label Text="{Binding PostModel.Contents}"/>
		</StackLayout>			
	</ContentView.Content>
</ContentView>
